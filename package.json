{
  "name": "daudr-blog",
  "private": true,
  "description": "Front end code for Daudr's blog",
  "version": "1.0.0",
  "author": "Michele Da Rin Fioretto <michele@daudr.me>",
  "bugs": {
    "url": "https://github.com/Daudr/daudr-blog/issues"
  },
  "dependencies": {
    "@material-ui/core": "^4.2.0",
    "gatsby": "^2.13.14",
    "gatsby-image": "^2.2.5",
    "gatsby-plugin-amp": "^0.2.2",
    "gatsby-plugin-disqus": "^1.1.1",
    "gatsby-plugin-feed": "^2.3.3",
    "gatsby-plugin-google-analytics": "^2.1.2",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-mailchimp": "^5.1.2",
    "gatsby-plugin-manifest": "^2.2.2",
    "gatsby-plugin-offline": "^2.2.2",
    "gatsby-plugin-react-helmet": "^3.1.1",
    "gatsby-plugin-sharp": "^2.2.4",
    "gatsby-plugin-sitemap": "^2.2.2",
    "gatsby-plugin-typography": "^2.3.1",
    "gatsby-remark-copy-linked-files": "^2.1.1",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.1.4",
    "gatsby-remark-prismjs": "^3.3.2",
    "gatsby-remark-responsive-iframe": "^2.2.2",
    "gatsby-remark-smartypants": "^2.1.1",
    "gatsby-source-filesystem": "^2.1.4",
    "gatsby-transformer-remark": "^2.6.3",
    "gatsby-transformer-sharp": "^2.2.2",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-cookie-consent": "^2.3.2",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-share": "^3.0.0",
    "react-typography": "^0.16.19",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.54",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "@testing-library/cypress": "^4.0.4",
    "axe-core": "^3.3.0",
    "cypress": "^3.4.0",
    "cypress-axe": "^0.5.1",
    "start-server-and-test": "^1.9.1",
    "auto-changelog": "^1.14.1",
    "babel-jest": "^24.8.0",
    "babel-preset-gatsby": "^0.2.2",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "firebase-tools": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.8.6",
    "semantic-release": "^15.13.18"
  },
  "homepage": "https://blog.daudr.me",
  "keywords": [
    "gatsby",
    "blog",
    "react"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "https://github.com/Daudr/daudr-blog.git"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "NODE_ENV=development && gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "deploy:test": "rm -fr public/ && gatsby build && firebase deploy --only hosting:test",
    "deploy:prod": "rm -fr public/ && gatsby build && firebase deploy --only hosting:prod",
    "cy:run": "npx cypress run",
    "cy:open": "cypress open",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run",
    "deploy:test:ci": "firebase deploy --only hosting:test --token=$FIREBASE_TOKEN",
    "deploy:prod:ci": "firebase deploy --only hosting:prod --token=$FIREBASE_TOKEN",
    "test": "jest",
    "test:ci": "jest --ci",
    "semantic-release": "semantic-release"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.2"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
